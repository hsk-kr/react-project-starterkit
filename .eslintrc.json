{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-console": 0,
    "react/jsx-one-expression-per-line": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
